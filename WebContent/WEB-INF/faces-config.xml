<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login/loginUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>empleado</display-name>
		<from-view-id>/login/loginUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>empleado</from-outcome>
			<to-view-id>/administracion/factura/listarFacturaPendiente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>admin</display-name>
		<from-view-id>/login/loginUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/administracion/menu/menuAdmin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>menuAdmin.xhtml</display-name>
		<from-view-id>/administracion/menu/menuAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login/loginUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarEmpresa</display-name>
		<from-view-id>/administracion/empresa/listarEmpresa.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login/loginUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarAgencia</display-name>
		<from-view-id>/administracion/agencia/listarAgencia.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login/loginUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>menuAdmin.xhtml</display-name>
		<from-view-id>/administracion/menu/menuAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>menu_listaEmpresa</from-outcome>
			<to-view-id>/administracion/empresa/listarEmpresa.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>menuAdmin.xhtml</display-name>
		<from-view-id>/administracion/menu/menuAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>menu_listaAgencia</from-outcome>
			<to-view-id>/administracion/agencia/listarAgencia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarEmpresa</display-name>
		<from-view-id>/administracion/empresa/listarEmpresa.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/administracion/agencia/listarAgencia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarEmpresa</display-name>
		<from-view-id>/administracion/empresa/listarEmpresa.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>empresa_bodega</from-outcome>
			<to-view-id>/administracion/bodega/listarBodega.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarAgencia</display-name>
		<from-view-id>/administracion/agencia/listarAgencia.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>agencia_bodega</from-outcome>
			<to-view-id>/administracion/bodega/listarBodega.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarBodega</display-name>
		<from-view-id>/administracion/bodega/listarBodega.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bodega_empresa</from-outcome>
			<to-view-id>/administracion/empresa/listarEmpresa.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarBodega</display-name>
		<from-view-id>/administracion/bodega/listarBodega.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bodega_agencia</from-outcome>
			<to-view-id>/administracion/agencia/listarAgencia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarAgencia</display-name>
		<from-view-id>/administracion/agencia/listarAgencia.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/administracion/empresa/listarEmpresa.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
     
<lifecycle> 
	<phase-listener>pfm.beans.cicloVida.MyPhaseListener</phase-listener>
</lifecycle>
</faces-config>
